{
  "name": "woth-toolbox",
  "version": "1.25.3",
  "private": true,
  "scripts": {
    "audit": "npm audit --all --recursive",
    "build": "next build",
    "dev": "next dev",
    "export:animals": "npx ts-node ./src/scripts/animals.ts > animals.json",
    "export:weapons": "npx ts-node ./src/scripts/weapons.ts > weapons.json",
    "https": "npx local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --fix",
    "start": "next start"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.3",
    "@svgr/webpack": "^8.1.0",
    "aws-amplify": "^6.0.16",
    "color": "^4.2.3",
    "get-user-locale": "^2.3.0",
    "loader-utils": "^3.2.1",
    "modern-normalize": "^2.0.0",
    "next": "^14.1.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-intl": "^6.6.2",
    "react-slider": "^2.0.6",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.4",
    "react-transition-group": "^4.4.5",
    "sha1": "^1.1.1",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-color": "^3.0.11",
    "@types/react-dom": "^18.2.19",
    "@types/react-slider": "^1.3.6",
    "@types/react-transition-group": "^4.4.10",
    "@types/sha1": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
